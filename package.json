{
  "name": "zle",
  "version": "0.9.2",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "git@github.com:zle-loves-e2e/zle.git",
  "author": "indexmotion <indexmotion@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/mocha": "^5.0.0",
    "mochawesome": "^3.0.2",
    "nyc": "^11.4.1",
    "typescript": "^2.6.2",
    "ts-node": "^5.0.1"
  },
  "dependencies": {
    "@types/lodash": "^4.14.104",
    "@types/puppeteer": "^1.0.0",
    "mocha": "^5.0.5",
    "puppeteer": "1.0.0"
  },
  "scripts": {
    "tsc": "./node_modules/.bin/tsc -p tsconfig.prod.json",
    "tsc:auto": "yarn run tsc --watch --watch-extensions ts",
    "test":
      "./node_modules/.bin/mocha --ui tdd -r ts-node/register --reporter mochawesome test/*.test.ts $ZLE_TEST_OPTIONS || true",
    "pretest": "./node_modules/.bin/ts-node ./test/TestServer start &",
    "posttest": "./node_modules/.bin/ts-node ./test/TestServer stop",
    "test:cov":
      "./node_modules/.bin/nyc --reporter=text --reporter=html yarn test",
    "release": "yarn test && yarn publish --registry http://registry.npmjs.org"
  }
}
